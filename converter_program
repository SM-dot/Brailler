  void one(char ch){
       Brailleconverter ob=new Brailleconverter();
       if(ch=='a')
           ob.a();
        if(ch=='b')
           ob.b();
        if(ch=='c')
           ob.c();
        if(ch=='d')
           ob.d();
        if(ch=='e')
           ob.e();
        if(ch=='f')
           ob.f();
        if(ch=='g')
           ob.g();
        if(ch=='h')
           ob.h();
        if(ch=='i')
           ob.i();
        if(ch=='j')
           ob.j();
        if(ch=='k')
           ob.k();
        if(ch=='l')
           ob.l();
        if(ch=='m')
           ob.m();
        if(ch=='n')
           ob.n();
        if(ch=='o')
           ob.o();
        if(ch=='p')
           ob.p();
        if(ch=='q')
           ob.q();
        if(ch=='r')
           ob.r();
        if(ch=='s')
           ob.s();
        if(ch=='t')
           ob.t();
        if(ch=='u')
           ob.u();
        if(ch=='v')
           ob.v();
        if(ch=='w')
           ob.w();
        if(ch=='x')
           ob.x();
