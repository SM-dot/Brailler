    public static void main(String[] args) {
        // TODO code application logic here
        Scanner A=new Scanner(System.in);
        Brailleconverter ob=new Brailleconverter();
        System.out.println("Enter a word or sentence toconvert to Braille. ");
        String st=A.nextLine();
        int len=st.length();
        
        
        for(int i=0;i<len;i++){
        char ch=st.charAt(i);
        ob.one(ch);
       
        if(ch==' '){
                System.out.print("  ");
              }
        
        }
        
        System.out.println();
        
        for(int i=0;i<len;i++){
        char ch=st.charAt(i);
        ob.two(ch);
        if(ch==' '){
                System.out.print("  ");
        }        
        }
        
        System.out.println();
        
        for(int i=0;i<len;i++){
        char ch=st.charAt(i);
        ob.three(ch);
        if(ch==' '){
                System.out.print("  ");
        }
        
        }
        
    }
    
}
